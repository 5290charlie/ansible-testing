[mysqld]

# These are commonly set, remove the # and set as required.
# basedir = .....
datadir = /db01/data
port = 3306
server_id = 100235
socket = /var/lib/mysql/mysql.sock

#skip-name-resolve

#====================
# Networking
#====================
# Need to change to internal address
# bind-address = 127.0.0.1
bind-address = 0.0.0.0
#bind-address = xxx.xxx.xxx.xxx

# Remove leading # and set to the amount of RAM for the most important data
# cache in MySQL. Start at 70% of total RAM for dedicated server, else 10%.

#===================
# Replication      #
# ==================
replicate-ignore-db=mysql
slave-skip-errors = 1062
log-slave-updates
relay-log = db56.ord-relay-bin
relay-log-index = db56.ord-relay-bin

#====================
# CACHES AND LIMITS #
#====================
tmp_table_size                 = 2048M
max_heap_table_size            = 2048M
query_cache_type               = 0
query_cache_size               = 64M
query_cache_limit              = 1M
#max_connections               = 4000
max_connections                = 3000
thread_cache_size              = 200
max_connect_errors             = 10000
concurrent_insert              = 2
connect_timeout                = 30
max_allowed_packet             = 32M
open_files_limit               = 8192
max_allowed_packet             = 32M
open_files_limit               = 8192
table_definition_cache         = 2048
#table_open_cache              = 2048
table_open_cache               = 4096   # Chgd by JSchroeter per mysqltuner
table_open_cache_instances     = 8      # Added per Documentation, previous value 1, default

# Close connection after idle 10 minutes
wait_timeout                   = 600
#join_buffer_size              = 8M
join_buffer_size               = 16M # Chg Per JSCHROETER per mysqltuner
sort_buffer_size               = 2M
#read_rnd_buffer_size          = 2M
read_rnd_buffer_size           = 8M  # Chgd JWS on 09/27/2013 per Doc

#====================
# MyISAM            #
#====================
#key_buffer_size               = 8192M
#key_buffer_size               = 256M   # Chgd JWS on 08/20/2013
key_buffer_size                = 64M    # Chgd JWS on 12/19/2013
#bulk_insert_buffer_size       = 2048M
bulk_insert_buffer_size        = 1024M  # Chgd JWS on 09/27/2013
#myisam_sort_buffer_size       = 2048M
#myisam_sort_buffer_size       = 1024M  # Chgd JWS on 09/27/2013
myisam_sort_buffer_size        = 16M    # Chgd JWS on 12/19/2013

#====================
# INNODB            #
#====================
#innodb_buffer_pool_size       = 8192M
#innodb_buffer_pool_size       = 16384M
#innodb_buffer_pool_size       = 22528M  # Chgd JWS on 08/20/2013 per mysqltuner
#innodb_buffer_pool_size       = 32768M  # Chgd JWS on 09/27/2013 per mysqltuner
#innodb_buffer_pool_size        = 49152M  # Chgd JWS on 12/19/2013 per mysqltuner
innodb_buffer_pool_size       = 32768M # Changed on 01/02/2014 per mysqltuner CSC
innodb_log_file_size           = 512M
innodb_log_buffer_size         = 32M
innodb_buffer_pool_instances   = 8
innodb_flush_method            = O_DIRECT
innodb_log_files_in_group      = 2
innodb_flush_log_at_trx_commit = 2
innodb_file_per_table          = 1
metadata_locks_hash_instances  = 16   # Added by JWS on 09/27/2013 per Doc
#innodb_thread_concurrency     = 12
#innodb_read_io_threads        = 8
#innodb_write_io_threads       = 8

# Remove leading # to turn on a very important data integrity option: logging
# changes to the binary log between backups.
log-bin = /db01/binlog/db56.ord-binlog
log-bin-index = /db01/binlog/db56.ord-binlog
expire_logs_days = 10

#====================
# LOGGING           #
#====================
general_log                    = 0
general_log_file               = /var/log/mysql/mysql-general.log
log_error                      = 1
log_error                      = /var/log/mysql/mysql-error.log
log_queries_not_using_indexes  = 1
log_throttle_queries_not_using_indexes = 60
slow_query_log                 = 1
long_query_time                = 10
slow_query_log_file            = /var/log/mysql/mysql-slow.log

# Commented out by JSCHROETER. Should Investigate Later
#sql_mode=NO_ENGINE_SUBSTITUTION,STRICT_TRANS_TABLES

# Will be depricated next release:
# http://dev.mysql.com/doc/refman/5.6/en/server-system-variables.html#sysvar_explicit_defaults_for_timestamp
explicit_defaults_for_timestamp=1
